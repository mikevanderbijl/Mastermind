public class Mastermind {
    
    //LOADER Class

    public static void main(String[] args){
        TerminalPlayer player = new TerminalPlayer();

        Game game = new Game(player, Integer.parseInt(args[0]), Integer.parseInt(args[1]), Integer.parseInt(args[2]));

        game.play();
    }
}
